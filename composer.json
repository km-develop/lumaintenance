{
  "name": "yasudacloud/lumaintenance",
  "description": "This is a library that supports Lumen Framework maintenance functions.",
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "laravel/lumen-framework": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Lumaintenance\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "archive": {
    "exclude": ["demo"]
  }
}
